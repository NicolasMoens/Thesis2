@article{Turner12001,
abstract = {A module for the ZEUS-2D code is described that may be used to solve the equations of radiation hydrodynamics to order unity in v/c, in the {\~{N}}ux-limited di usion (FLD) approximation. In this approx-imation, the factor Eddington tensor f, which closes the radiation moment equations, is chosen to be an empirical function of the radiation energy density. This is easier to implement and faster than full-transport techniques, in which f is computed by solving the transfer equation. However, FLD is less accurate when the {\~{N}}ux has a component perpendicular to the gradient in radiation energy density and in optically thin regions when the radiation Ðeld depends strongly on angle. The material component of the {\~{N}}uid is here assumed to be in local thermodynamic equilibrium. The energy equations are operator split, with transport terms, radiation di usion term, and other source terms evolved separately. Transport terms are applied using the same consistent transport algorithm as in ZEUS-2D. The radiation di usion term is updated using an alternating direction{\`{E}}implicit method with convergence checking. Remaining source terms are advanced together implicitly using numerical root Ðnding. However, when absorption opacity is zero, accuracy is improved by instead treating the compression and expansion source terms using a time-centered di erencing scheme. Results are discussed for test problems including radiation-damped linear waves, radiation fronts propagating in optically thin media, subcritical and supercritical radiating shocks, and an optically thick shock in which radiation dominates downstream pressure.},
author = {Turner1, N J and Stone1, J M},
file = {:home/nicolas/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Turner1, Stone1 - 2001 - A MODULE FOR RADIATION HYDRODYNAMIC CALCULATIONS WITH ZEUS-2D USING FLUX-LIMITED DIFFUSION(2).pdf:pdf},
journal = {THE ASTROPHYSICAL JOURNAL SUPPLEMENT SERIES},
keywords = {Subject headings,hydrodynamics {\`{E}} methods,numerical {\`{E}} radiative transfer},
pages = {95--107},
title = {{A MODULE FOR RADIATION HYDRODYNAMIC CALCULATIONS WITH ZEUS-2D USING FLUX-LIMITED DIFFUSION}},
url = {http://iopscience.iop.org/article/10.1086/321779/pdf},
volume = {135},
year = {2001}
}
@article{TheLIGOScientificCollaboration2016,
abstract = {On September 14, 2015 at 09:50:45 UTC the two detectors of the Laser Interferometer Gravitational-Wave Observatory simultaneously observed a transient gravitational-wave signal. The signal sweeps upwards in frequency from 35 to 250 Hz with a peak gravitational-wave strain of {\$}1.0 \backslashtimes 10{\^{}}{\{}-21{\}}{\$}. It matches the waveform predicted by general relativity for the inspiral and merger of a pair of black holes and the ringdown of the resulting single black hole. The signal was observed with a matched-filter signal-to-noise ratio of 24 and a false alarm rate estimated to be less than 1 event per 203 000 years, equivalent to a significance greater than 5.1 {\{}$\backslash$sigma{\}}. The source lies at a luminosity distance of {\$}410{\^{}}{\{}+160{\}}{\_}{\{}-180{\}}{\$} Mpc corresponding to a redshift {\$}z = 0.09{\^{}}{\{}+0.03{\}}{\_}{\{}-0.04{\}}{\$}. In the source frame, the initial black hole masses are {\$}36{\^{}}{\{}+5{\}}{\_}{\{}-4{\}} M{\_}\backslashodot{\$} and {\$}29{\^{}}{\{}+4{\}}{\_}{\{}-4{\}} M{\_}\backslashodot{\$}, and the final black hole mass is {\$}62{\^{}}{\{}+4{\}}{\_}{\{}-4{\}} M{\_}\backslashodot{\$}, with {\$}3.0{\^{}}{\{}+0.5{\}}{\_}{\{}-0.5{\}} M{\_}\backslashodot c{\^{}}2{\$} radiated in gravitational waves. All uncertainties define 90{\%} credible intervals.These observations demonstrate the existence of binary stellar-mass black hole systems. This is the first direct detection of gravitational waves and the first observation of a binary black hole merger.},
