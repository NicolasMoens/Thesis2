@article{Levermore1981,
abstract = {A diffusion theory for radiative transfer is derived which is naturally flux limited, i.e., the magnitude of the flux can be no greater than the density times the maximum transport speed. Numerical comparisons with exact solutions of the equation of transfer indicate that this approxi-mate theory is significantly more accurate than classical isotropic diffusion theory (the Eddington approximation) and asymptotic diffusion theory. Subject heading: radiative transfer In many problems involving transport theory one is often forced by computational or cost considerations to adopt some approximate method to handle the transport phenomena. By far the most common such methods in use today involve approximating the angular information by various forms of the diffusion (differential) approximation, typically based upon equihbrium diffusion theory (EDT, Pomraning 1973), isotropic diffusion theory (IDT, Larsen and Keller 1974), or asymptotic diffusion theory (ADT, Pomraning 1973; Case, de Hoffmann, and Placzek 1953; Winslow 1968). All these theories impose restrictions on significant spatial variations over distances of a mean free path, and all yield expressions for the flux of energy or particles which can violate causality (the magnitude of the flux can be no greater than the density times the maximum transport speed) when these restrictions are exceeded. While these constraints are reasonable for many physical problems, they are often violated for radiative transfer problems that arise in astrophysics and plasma physics. In the past this has led to the ad hoc introduction of a flux-hmiter (Winslow 1968; Lund and Wilson 1980), usually with some degree of arbitrariness, in order to preserve causahty in the presence of large spatial gradients. It is the purpose of this paper to present a diffusion theory which is naturally flux-hmited, and thus relaxes some of the restrictions usually necessary for the valid application of diffusion theory. We will refer to this approximate transport description as flux-limited diffusion theory (FDT). The setting of what follows will be radiative transfer, although the analysis makes no special use of this fact and is equally applicable to any one-speed transport problem. The generalization to frequency dependent (multigroup) problems can be accomplished as Winslow (1968) did in the ADT setting. For simplicity, only the case of isotropic elastic (coherent) scattering will be treated here, but the procedure can be applied to a more general scattering description. The outline of the remainder of this paper is as follows: The next section derives the flux-hmited diffusion equation, and discusses various limiting forms of the theory, as well as its connection with IDT and ADT. The FDT equation obtained is nonlinear, but we show in III that certain problems are solvable in closed analytic form. Section IV discusses the initial and boundary conditions that are necessary to complete the theory, and numerical comparisons of IDT, ADT, and FDT with exact transport results are presented in V. We close with a few summary remarks in the where r, Q, and t are the spatial, angular, and temporal variables, c is the speed of light, a 0 (r, t) is the absorption coefficient, {\textless}j s (r, t) is the scattering coefficient, o—o a +{\textless}j 5 is the total interaction coefficient, Z?(r, t) is the blackbody energy density, and E(r,t) is the radiation energy density as defined by},
author = {Levermore, G D and Pomraning, G C},
file = {::},
journal = {The Astrophysical Journal},
pages = {321--334},
title = {{A FLUX-LIMITED DIFFUSION THEORY}},
url = {http://articles.adsabs.harvard.edu/cgi-bin/nph-iarticle{\_}query?1981ApJ...248..321L{\&}data{\_}type=PDF{\_}HIGH{\&}whole{\_}paper=YES{\&}type=PRINTER{\&}filetype=.pdf},
volume = {248},
year = {1981}
}
